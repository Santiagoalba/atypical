
<div class="form_container">

    <div class="atypicalForm">

    <div class="atypicalForm_title">ATYPICAL</div>

    <form action="/addCoach" method="POST" class="formulario" enctype="multipart/form-data">
        <div class="atypicalForm_input">
            <input type="text" name="name" placeholder="Nombre">
        </div>
        <div class="atypicalForm_input">
            <input type="text" name="surname" placeholder="Apellido">
        </div>
        <div class="atypicalForm_input">
            <input type="text" name="ign"  placeholder="Nick">
        </div>
        <div class="atypicalForm_input">
            <input type="text" name="description" placeholder="Descripcion">
        </div>
        <div class="atypicalForm_input">
            <input type="text" name="game" placeholder="Juego">
        </div>
        <div class="atypicalForm_input">
            <input type="number" name="price" placeholder="Precio">
        </div>
        
            <div class="atypicalForm_input"><label for="">Coach photo</label></div>
            <input type="file" class="img_input" name="img" placeholder="Foto">
        
            {{!-- <div class="signInForm_file"><label for="">Team logo</label></div>
            <input type="file"  name="imglogo" placeholder="Logo"> --}}
        
        <div class="formButton">
            <button type="submit">Add Coach</button>
        </div>
    </form>
</div>
</div>

